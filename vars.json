									"include_in_parent" : true,
									"include_in_root" : true
								}
							}
						}
					}
				},
				"userAppendedData" : {
					"type" : "nested",
					"include_in_parent" : true,
					"include_in_root" : true,
					"properties" : {
						"associatedForms" : {
							"type" : "nested",
							"include_in_parent" : true,
							"include_in_root" : true
						}
					}
				}
			}
		},
		"genealogy" : {
			"properties" : {
				"createdOnDevice" : {
					"type" : "string",
					"store" : true,
					"index" : "not_analyzed"
				},
				"dateCreated" : {
					"type" : "long",
					"store" : true
				},
				"hashes" : {
					"type" : "string",
					"store" : true,
					"index" : "not_analyzed"
				}
			}
