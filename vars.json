	"ELASTICSEARCH_MAPPINGS" : {
		"public_hash" : {
			"type" : "string",
			"store" : true,
			"index" : "not_analyzed"
		},
		"data" : {
			"properties" : {
				"exif" : {
					"properties" : {
						"location" : {
							"type" : "geo_point",
							"store" : true,
							"include_in_parent" : true,
							"include_in_root" : true
						}
					}
				},
				"sensorCapture" : {
					"type" : "nested",
					"include_in_parent" : true,
					"include_in_root" : true,
					"properties" : {
						"sensorPlayback" : {
							"type" : "nested",
							"include_in_parent" : true,
